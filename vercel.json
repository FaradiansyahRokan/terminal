{
  "rewrites": [
    {
      "source": "/api/binance/(.*)",
      "destination": "/api/binance.js"
    },
    {
      "source": "/api/finnhub/(.*)",
      "destination": "/api/finnhub.js"
    },
    {
      "source": "/api/yahoo-chart",
      "destination": "/api/yahoo-chart.js"
    },
    {
      "source": "/api/yahoo-search",
      "destination": "/api/yahoo-search.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}