{
  "rewrites": [
    {
      "source": "/api/yahoo-chart/:match*",
      "destination": "https://query1.finance.yahoo.com/v8/finance/chart/:match*"
    },
    {
      "source": "/api/yahoo-search/:match*",
      "destination": "https://query2.finance.yahoo.com/v1/finance/search/:match*"
    },
    {
      "source": "/api/finnhub/:match*",
      "destination": "https://finnhub.io/api/v1/:match*"
    },
    {
      "source": "/api/binance/:match*",
      "destination": "https://data-api.binance.vision/api/v3/:match*"
    }
  ]
}